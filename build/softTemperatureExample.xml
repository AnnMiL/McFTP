<?xml version="1.0" ?>
<processor>


</processor>


<experiment name="soft_temperature_sensor_test">
	<duration unit="sec" value="30"/>
	<save_result value="True"/>
	<processor core_number="2" fixed_active="False" fixed_frequency="True"/>
	<temperature_sensors>
		<sensor0 type = "hardware" />
		<sensor1 type = "soft" calculator = "default" parameterW = ""  parameterV = "" parameterK = "">
			<thermal_model>
				<parameter name= "C" path = ""/>
				<parameter name= "G" path = ""/>
				<parameter name= "K" path = ""/>
				<parameter name= "ambient temperature" value = ""/> 

			</thermal_model>
		</sensor1>

	</temperature_sensors>

	<soft_temperature_sensor_counters>
		<!-- <sensor0 name="perf::PERF_COUNT_HW_CPU_CYCLES:u=0" coefficient_a="1" coefficient_b = "0" /> -->
		<!-- <sensor1 name="PAPI_L2_DCA" coefficient_a="1" coefficient_b = "0" /> -->
		<!-- <sensor2 name="UOPS_EXECUTED" coefficient_a="1" coefficient_b = "0" /> -->
		<sensor1 coefficient_a="1" coefficient_b="0" name="ix86arch::UNHALTED_CORE_CYCLES"/>
   <sensor2 coefficient_a="1" coefficient_b="0" name="ix86arch::INSTRUCTION_RETIRED"/>
   <sensor3 coefficient_a="1" coefficient_b="0" name="ix86arch::UNHALTED_REFERENCE_CYCLES"/>
   <sensor4 coefficient_a="1" coefficient_b="0" name="ix86arch::LLC_REFERENCES"/>
   <sensor5 coefficient_a="1" coefficient_b="0" name="ix86arch::LLC_MISSES"/>
   <sensor6 coefficient_a="1" coefficient_b="0" name="ix86arch::BRANCH_INSTRUCTIONS_RETIRED"/>
   <sensor7 coefficient_a="1" coefficient_b="0" name="ix86arch::MISPREDICTED_BRANCH_RETIRED"/>
   <sensor8 coefficient_a="1" coefficient_b="0" name="perf::PERF_COUNT_HW_CPU_CYCLES"/>
   <sensor9 coefficient_a="1" coefficient_b="0" name="perf::CYCLES"/>
   <sensor10 coefficient_a="1" coefficient_b="0" name="perf::CPU-CYCLES"/>
   <sensor11 coefficient_a="1" coefficient_b="0" name="perf::PERF_COUNT_HW_INSTRUCTIONS"/>
   <sensor12 coefficient_a="1" coefficient_b="0" name="perf::INSTRUCTIONS"/>
   <sensor13 coefficient_a="1" coefficient_b="0" name="perf::PERF_COUNT_HW_CACHE_REFERENCES"/>
   <sensor14 coefficient_a="1" coefficient_b="0" name="perf::CACHE-REFERENCES"/>
   <sensor15 coefficient_a="1" coefficient_b="0" name="perf::PERF_COUNT_HW_CACHE_MISSES"/>
   <sensor16 coefficient_a="1" coefficient_b="0" name="perf::CACHE-MISSES"/>
   <sensor17 coefficient_a="1" coefficient_b="0" name="perf::PERF_COUNT_HW_BRANCH_INSTRUCTIONS"/>
   <sensor18 coefficient_a="1" coefficient_b="0" name="perf::BRANCH-INSTRUCTIONS"/>
   <sensor19 coefficient_a="1" coefficient_b="0" name="perf::BRANCHES"/>
   <sensor20 coefficient_a="1" coefficient_b="0" name="perf::PERF_COUNT_HW_BRANCH_MISSES"/>
	</soft_temperature_sensor_counters>
	<tasks>
		<task0 load_type="busy_wait" name="task0" periodicity="periodic" type="singlecore">
			<period unit="ms" value="1000"/>
			<wcets unit="ms" value="[999]"/>
			<attached_core value="0"/>
		</task0>
		<task1 load_type="busy_wait" name="task1" periodicity="periodic" type="singlecore">
			<period unit="ms" value="1000"/>
			<wcets unit="ms" value="[999]"/>
			<attached_core value="1"/>
		</task1>
		
	</tasks>

	
	
	<thermal_approach>
		<kernel type="PBOO">
			<ton unit="ms" value="{15000, 15000}"/>
			<toff unit="ms" value="{10000, 10000}"/>
		</kernel>
	</thermal_approach>
</experiment>
